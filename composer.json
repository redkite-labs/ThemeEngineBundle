{
  "name": "alphalemon/theme-engine-bundle",
  "description": "ThemeEngineBundle adds theming feature to any Symfony2 application, adding themes packed as standard Symfony2 Bundles",
  "keywords": ["theme", "themes", "cms"],
  "homepage": "http://alphalemon.com",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
  ],
  "require": {
    "symfony/symfony":  ">=2.0.9,<2.1.0-dev",
    "propel/propel-bundle": ">=1.6.0,<2.0.*-dev",
    "alphalemon/al-pagetree-bundle": "",
    "alphalemon/al-valum-uploader-bundle": ""
  },
  "autoload": {
    "psr-0": { "ThemeEngineCore": "vendor/bundles/AlphaLemon/ThemeEngineBundle/src",
               "Themes": "vendor/bundles/AlphaLemon/ThemeEngineBundle",
  },
  "target-dir": "AlphaLemon/ThemeEngineBundle"
}
