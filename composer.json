  "keywords": ["theme", "themes", "cms"],
  "homepage": "http://alphalemon.com",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
  ],
  "require": {
    "symfony/symfony":  ">=2.0.10,<2.1.0-dev",
    "propel/propel-bundle": "2.0-dev",
    "alphalemon/al-pagetree-bundle": "master-dev",
    "alphalemon/al-valum-uploader-bundle": "master-dev"
  },
  "autoload": {
    "psr-0": { "AlphaLemon\\ThemeEngineBundle": "",
               "Themes": "Themes/"
    }
  }
